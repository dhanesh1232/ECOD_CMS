{
  "theme": {
    "name": "ECOD",
    "theme_name": "ecod_services",
    "theme_author": "Dhanesh"
  },
  "settings": {
    "max_width": "1500px",
    "page_width": "1300px",
    "background_color": "#703e3e",
    "text_color": "#d10000",
    "font_family": "Arial, sans-serif",
    "primary_color": "#decece",
    "secondary_color": "#6c757d",
    "accent_color": "#b2c7b7",
    "link_color": "#007bff",
    "button": {
      "background_color": "#d96d6d",
      "text_color": "#ffffff",
      "border_radius": "6px",
      "padding": "22",
      "hover": "#3d0021",
      "shadow": "0px 4px 21px rgba(0,0,0,0.2)"
    },
    "typography": {
      "heading_font": "Poppins, sans-serif",
      "body_font": "Roboto, sans-serif",
      "heading_size": "24px",
      "body_size": "16px",
      "line_height": "1.6"
    },
    "layout": {
      "container_max_width": "1200px",
      "section_padding": "10",
      "card_border_radius": "10px",
      "card_shadow": "0px 4px 8px rgba(0, 0, 0, 0.1)"
    }
  },
  "sections": {
    "header": {
      "background_color": "#ffffff",
      "text_color": "#333333",
      "logo_size": "180px",
      "nav_link_color": "#007bff",
      "nav_hover_color": "#0056b3"
    },
    "footer": {
      "background_color": "#333333",
      "text_color": "#ffffff",
      "link_color": "#f8f9fa"
    },
    "cards": {
      "background_color": "#0080ff",
      "text_color": "#333333",
      "border_radius": "8px",
      "shadow": "0px 4px 6px rgba(0, 0, 0, 0.1)"
    }
  }
}
